    {
      "parameters": {
        "jsCode": "for (const item of items) {\n  let opportunities = item.json.opportunities || (item.json.json && item.json.json.opportunities);\n  if (!opportunities || !Array.isArray(opportunities) || opportunities.length === 0) {\n    item.json = { formattedOutput: 'No opportunities data available.' };\n    continue;\n  }\n  let output = '--- Opportunity Report ---\\n\\n';\n  opportunities.forEach((opportunity, index) => {\n    output += `Opportunity #${index + 1}\\n` +\n      '--------------------------\\n' +\n      `Project Name: ${opportunity.name || 'N/A'}\\n` +\n      `Status: ${opportunity.status || 'N/A'}\\n` +\n      `Value: $${opportunity.monetaryValue !== undefined ? opportunity.monetaryValue : 'N/A'}\\n`;\n    const contact = opportunity.contact;\n    if (contact) {\n      output += '\\nContact:\\n' +\n        `  Name: ${contact.name || 'N/A'}\\n` +\n        `  Company: ${contact.companyName || 'N/A'}\\n` +\n        `  Email: ${contact.email || 'N/A'}\\n` +\n        `  Phone: ${contact.phone || 'N/A'}\\n`;\n      if (contact.tags && contact.tags.length > 0) {\n        output += `  Tags: ${contact.tags.join(', ')}\\n`;\n      }\n    }\n    const incompleteTasks = opportunity.tasks ? opportunity.tasks.filter(task => !task.completed) : [];\n    if (incompleteTasks.length > 0) {\n      output += '\\nIncomplete Tasks:\\n';\n      incompleteTasks.forEach(task => {\n        const dueDate = task.dueDate ? new Date(task.dueDate).toLocaleDateString('en-US', { dateStyle: 'medium' }) : 'N/A';\n        output += `  - Title: ${task.title || 'N/A'}\\n` +\n          `    Due Date: ${dueDate}\\n` +\n          `    Description: ${task.description || 'N/A'}\\n`;\n      });\n    } else {\n      output += '\\nIncomplete Tasks: None\\n';\n    }\n    output += '\\n\\n';\n  });\n  item.json = { formattedOutput: output.trim() };\n}\nreturn items;"
      },
